<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
	<!-- Property to read for device identifier -->
	<string name="property_device">ro.2by2.device</string>

	<!-- %s is expanded to property_version value. .zip, .delta, .update, .sign extensions added when needed -->
	<string name="filename_base">2by2-Project-%s</string>

	<!-- URL base to find .update and .sign files, %s is expanded to property_device value. Path is suffixed by (expanded) filename_base -->
	<string name="url_base_update">https://project2by2.jp/builds/%s/15/</string>

	<!-- URL base to find the full zip files, %s is replaced to be property_device value. -->
	<string name="url_base_full">https://project2by2.jp/builds/%s/15/</string>

	<!-- URL base to find the full zip sha256sum files, %s is replaced to be property_device value. -->
	<string name="url_base_full_sum">https://project2by2.jp/builds/%s/15/</string>

	<!-- the branch name to format URLs with -->
	<string name="url_branch_name">Vanadium</string>

	<!-- URL base to fetch all builds. first %s is replaced by branch name later on by property_device -->
	<string name="url_base_json" formatted="false">https://raw.githubusercontent.com/2by2-Project/vendor_2by2-ota/%s/%s/%s.json</string>

	<!-- github API url, pointing to the OTA json repo. %s formatted as described above -->
	<string name="url_api_history" formatted="false">https://api.github.com/repos/2by2-Project/vendor_2by2-ota/commits?sha=%s&amp;path=%s/%s.json</string>

	<!-- URL base to fetch cert jsons. first %s is replaced by branch name second by property_device -->
	<string name="url_cert_json" formatted="false">https://raw.githubusercontent.com/2by2-Project/vendor_2by2-ota/%s/%s/cert.json</string>
</resources>
